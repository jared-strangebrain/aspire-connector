{
	"name": "aspire",
	"version": "1.0.0",
	"description": "Aspire",
	"engines": {
		"node": ">=18.x"
	},
	"exports": {
		"./*": "./dist/*.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*"
			]
		}
	},
	"files": [
		"/dist"
	],
	"dependencies": {
		"@trayio/cdk-dsl": "*",
		"@trayio/cdk-runtime": "*"
	},
	"devDependencies": {
		"@types/jest": "28.1.3",
		"@types/node": "*",
		"@typescript-eslint/eslint-plugin": "*",
		"jest": "29.6.1",
		"ts-jest": "29.1.1",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"typescript-cp": "0.1.8",
		"eslint": "8.57.0",
		"eslint-config-airbnb-base-typescript": "1.1.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-prettier": "4.2.1",
		"prettier": "2.8.3"
	},
	"scripts": {
		"reinstall": "rm package-lock.json && rm -rf node_modules && npm cache clean --force && npm install",
		"clean": "rm -rf dist && rm tsconfig.tsbuildinfo",
		"compile": "tsc --build && tscp >> /dev/null",
		"test": "jest --config ./jest.config.js",
		"lint:fix": "eslint --fix ./src"
	}
}
